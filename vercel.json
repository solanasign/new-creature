{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.jsx",
      "use": "@vercel/node"
    },
    {
      "src": "*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "*.tsx",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "cleanUrls": true
}